class OprahsFavoriteThings::Product

  attr_accessor :title, :description, :price, :image, :retailer, :url

  def initialize
    @title = title
    @description = description
    @price = price
    @image = image
    @retailer = retailer
    @url = url
  end

  def attributes
    {
    title: @title,
    description: @description,
    price: @price,
    image: @image,
    retailer: @retailer,
    url: @url
    }
    end
end 
